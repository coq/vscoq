{
  "Abstract": {
    "prefix": "abstract",
    "body": ["abstract ${1}.\n$0"],
    "description": "abstract $expr."
  },
  "Abstract Using": {
    "prefix": "abstract",
    "body": ["abstract ${1} using ${2}.\n$0"],
    "description": "abstract $expr using $ident."
  },
  "Theorem": {
    "prefix": "Theorem",
    "body": [
      "Theorem ${1}: ${2}.",
      "Proof.",
      "\t$0",
      "Qed.\n"
      ],
    "description": "Theorem..."
  },
  "Definition": {
    "prefix": "Definition",
    "body": ["Definition $1 := $2.\n"],
    "description": "Definition..."
  },
  "Notation (Simple)": {
    "prefix": "Notation",
    "body": [ "Notation \"$1\" := ($2).\n$0" ],
    "description": "Notation"
  },
  "Notation (Full)": {
    "prefix": "Notation",
    "body": [
      "Notation \"$1\" :=",
      "\t($2)",
      "\t(at level $3, ${4:[no|left|right]} associativity)",
      "\t: ${5:scope}.\n"
    ],
    "description": "Notation, level, associativity, scope"
  },
  "Section": {
    "prefix": "Section",
    "body": [
      "Section ${name}.",
      "\t$0",
      "End ${name}.\n"
      ],
    "description": "Section..."
  },
  "Module": {
    "prefix": "Module",
    "body": [
      "Module ${name}.",
      "\t$0",
      "End ${name}.\n"
      ],
    "description": "Module..."
  },
  "Obligation Tactic": {
    "prefix": "Obligation Tactic",
    "body": ["Obligation Tactic := ${expr}."]
  },
  "Local Obligation Tactic": {
    "prefix": "Local Obligation Tactic",
    "body": ["Local Obligation Tactic := ${expr}."]
  },
  "Global Obligation Tactic": {
    "prefix": "Global Obligation Tactic",
    "body": ["Global Obligation Tactic := ${expr}."]
  },
  "Show Obligation Tactic": {
    "prefix": "Show Obligation Tactic",
    "body": ["Show Obligation Tactic."]
  },
  "Obligations": {
    "prefix": "Obligations",
    "body": ["Obligations."]
  },
  "Obligations of...": {
    "prefix": "Obligations of",
    "body": ["Obligations of ${ident}."]
  },
  "Obligation (num)": {
    "prefix": "Obligation",
    "body": ["Obligation ${num}."]
  },
  "Obligation (num) of...": {
    "prefix": "Obligation of",
    "body": ["Obligation ${num} of ${ident}."]
  },
  "Next Obligation": {
    "prefix": "Next Obligation",
    "body": ["Next Obligation."]
  },
  "Next Obligation of...": {
    "prefix": "Next Obligation of",
    "body": ["Next Obligation of ${ident}."]
  },
  "Solve Obligation": {
    "prefix": "Solve Obligation",
    "body": ["Solve Obligation."]
  },
  "Solve Obligation of...": {
    "prefix": "Solve Obligation of",
    "body": ["Solve Obligation of ${ident}."]
  },
  "Solve Obligation of... with...": {
    "prefix": "Solve Obligation of",
    "body": ["Solve Obligation of ${ident} with ${expr}."]
  },
  "Solve All Obligations": {
    "prefix": "Solve All Obligations",
    "body": ["Solve All Obligations."]
  },
  "Solve All Obligations with...": {
    "prefix": "Solve All Obligations with",
    "body": ["Solve All Obligations with ${expr}."]
  },
  "Admit Obligations": {
    "prefix": "Admit Obligations",
    "body": ["Admit Obligations."]
  },
  "Admit Obligations of...": {
    "prefix": "Admit Obligations of...",
    "body": ["Admit Obligations of ${ident}."]
  },
  "Preterm": {
    "prefix": "Preterm",
    "body": ["Preterm."]
  },
  "Preterm of...": {
    "prefix": "Preterm of",
    "body": ["Preterm of ${ident}."]
  }

}