          "dark": "#7cfc00",
          "light": "#32cd32",
          "highContrast": "#32cd32"
        }
      },
      {
        "id": "coq.termType",
        "description": "Color of types in terms.",
        "defaults": {
          "dark": "#f08080",
          "light": "#ff7f50",
          "highContrast": "#ff7f50"
        }
      },
      {
        "id": "coq.termVariable",
        "description": "Color of variables in terms.",
        "defaults": {
          "dark": "#7fff00",
          "light": "#20b2aa",
          "highContrast": "#20b2aa"
        }
